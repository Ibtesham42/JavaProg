import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        String n;

        Scanner sc = new Scanner(System.in);
        System.out.println("Enter Your String Value");
        n = sc.nextLine();
        int no = n.length();
        String rev = "";
        for(int i = no-1; i>=0;i--)
        {
            rev = rev +n.charAt(i);
        }
        System.out.println(rev);

    }
}
